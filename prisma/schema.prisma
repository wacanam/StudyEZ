generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["postgresqlExtensions"]
}

datasource db {
  provider   = "postgresql"
  extensions = [vector]
}

model Document {
  id        Int      @id @default(autoincrement())
  content   String
  metadata  Json     @default("{}")
  embedding Unsupported("vector(768)")?
  createdAt DateTime @default(now()) @map("created_at")

  @@map("documents")
}
